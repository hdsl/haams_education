<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.org/prettyfaces/3.3.3" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.org/prettyfaces/3.3.3
               http://ocpsoft.org/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">

    <url-mapping id="login">
        <pattern value="/login"></pattern>
        <view-id>/haams/index.xhtml</view-id>
    </url-mapping>    
    
    <url-mapping id="home">
        <pattern value="/home"></pattern>
        <view-id>/haams/template.xhtml</view-id>
    </url-mapping>
    
    <url-mapping id="student_entry">
        <pattern value="/sysadmin/student_entry/new"></pattern>
        <view-id>/haams/sysadmin/student_reg.xhtml</view-id>
    </url-mapping>
    
    <url-mapping id="module">
        <pattern value="/#{navHandler.module}/#{navHandler.page}"></pattern>
        <view-id>/haams/template.xhtml</view-id>
    </url-mapping>
    
    <url-mapping id="clientDetailEdit">
        <pattern value="/sysadmin/client_detail/edit"></pattern>
        <view-id>/hysen/sysadmin/client_detail_view.xhtml</view-id>
    </url-mapping>
    
</pretty-config>
